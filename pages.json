{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/map/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/find/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/activity/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/me/index",
			"style": {
				"navigationStyle": "custom"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"tabBar": {
		"color": "#c4c4c4", // tab 上的文字默认颜色
		"selectedColor": "#1a1a1a", // tab 上的文字选中时的颜色
		"borderStyle": "white", // tabbar 上边框的颜色，可选值 black/white
		"backgroundColor": "#ffffff", // tab 的背景色
		"iconWidth": "26px", //图标默认宽度（高度等比例缩放）	
		"height": "60px", //tabBar 默认高度
		"fontSize": "12px", //文字默认大小	
		"list": [{ // 最少2个、最多5个 tab
			"pagePath": "pages/find/index", // 页面路径，必须在 pages 中先定义
			"iconPath": "static/images/icon-find.png", // 图片路径，icon 大小限制为40kb，建议尺寸为 81px * 81px
			"selectedIconPath": "static/images/icon-find-on.png", // 选中时的图片路径
			"text": "发现" // tab 上按钮文字
		}, {
			"pagePath": "pages/map/index",
			"iconPath": "static/images/icon-adress.png",
			"selectedIconPath": "static/images/icon-adress-on.png",
			"text": "地图"
		}, {
			"pagePath": "pages/index/index",
			"iconPath": "static/images/icon-tesla.png",
			"selectedIconPath": "static/images/icon-tesla-on.png",
			"text": "首页"
		}, {
			"pagePath": "pages/activity/index",
			"iconPath": "static/images/icon-huodong.png",
			"selectedIconPath": "static/images/icon-huodong-on.png",
			"text": "活动"
		}, {
			"pagePath": "pages/me/index",
			"iconPath": "static/images/icon-me.png",
			"selectedIconPath": "static/images/icon-me-on.png",
			"text": "我的"
		}]
	},
	"uniIdRouter": {}
}
